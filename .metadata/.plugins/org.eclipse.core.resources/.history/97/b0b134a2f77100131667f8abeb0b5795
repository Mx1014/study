
public class TestVolate {

	 int b = 5;
	
	
	public TestVolate(){
		new Thread1().start();
		new Thread1().start();
	}
	
	class Thread1 extends Thread{
		
		@Override
		public void run(){
			System.out.println(b);
			b++;
			System.out.println(b);
		}
	}
	
	public static void main(String[] args) {
		new TestVolate();
	}
}

